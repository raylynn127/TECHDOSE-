#include<stdio.h>
int main()
{
    int n;
    printf("Ente the number till which you want to print the prime numbers\n");
    scanf("%d",&n);
    printf("The prime numbers are-\n");
    for(int i=2;i<=n;i++){
        int c=0;
        for(int j=1;j<=i;j++){
            if(i%j==0){
                c++;
            }
        }
        if(c==2){
            printf("%d ",i);
        }
    }
}
